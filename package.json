{
  "name": "b58uuid",
  "version": "1.0.0",
  "description": "High-performance Base58-encoded UUID library for JavaScript/TypeScript with zero dependencies",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "type-check": "tsc --noEmit",
    "benchmark": "npm run build && node benchmarks/performance.js",
    "prepublishOnly": "npm run build && npm run test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/b58uuid/b58uuid-js.git"
  },
  "homepage": "https://b58uuid.io",
  "bugs": {
    "url": "https://github.com/b58uuid/b58uuid-js/issues"
  },
  "keywords": [
    "uuid",
    "base58",
    "short-uuid",
    "encoding",
    "javascript",
    "typescript",
    "zero-dependencies",
    "url-safe",
    "compact-uuid"
  ],
  "author": "b58uuid contributors",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.3.0"
  }
}
